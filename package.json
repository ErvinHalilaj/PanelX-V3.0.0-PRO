    "db:seed": "tsx server/scripts/seed.ts",
    "db:studio": "drizzle-kit studio",
    "check": "tsc",
    "clean": "rm -rf dist node_modules/.vite",
    "install:all": "npm install && npm install tsx dotenv -g"
